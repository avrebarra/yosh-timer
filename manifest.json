{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Yosh! Timer",
    "short_name": "Yosh Timer",
    "description": "Simple timer app to track time of, like, everything!",
    "author": "Avreghly Barra (@shrotavre)",
    "version": "0.3",
    "manifest_version": 2,
    "permissions": ["storage"],
    "app": {
        "background": {
            "scripts": ["invoker.js"]
        }
    },
    "offline_enabled": true,
    "icons": { "16": "res/img/icons/timie-16.png", "128": "res/img/icons/timie-128.png" },
    "minimum_chrome_version": "33"
}